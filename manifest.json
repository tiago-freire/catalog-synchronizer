{
  "name": "catalog-synchronizer-admin-prototype",
  "vendor": "ssesandbox04",
  "version": "0.0.1",
  "title": "Catalog Synchronizer Admin (Prototype)",
  "description": "Catalog Synchronizer captures product changes through an EndPoint that receives Affiliate Notifications. Received notifications trigger Catalog changes synchronization to third-party systems such as Nosto or Algolia.",
  "dependencies": {
    "vtex.styleguide": "9.x"
  },
  "builders": {
    "react": "3.x",
    "node": "6.x",
    "admin": "0.x",
    "messages": "1.x",
    "docs": "0.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/{{account}}/pricing/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/catalog_system/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/profile-system/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "kubernetes.default.svc.cluster.local",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "infra.io.vtex.com",
        "path": "*"
      }
    },
    {
      "name": "Sku.aspx"
    },
    {
      "name": "vtex.messages:translate-messages"
    },
    {
      "name": "read_prices"
    },
    {
      "name": "ADMIN_CRM"
    },
    {
      "name": "POWER_USER_CRM"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*",
        "path": "*"
      }
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
